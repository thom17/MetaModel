@startuml
group UC01 TypingSystem
	group playTyping()
		TypingSystem->TypingSystem : parameterSet()
	TypingSystem-->TypingSystem : 
	end
	TypingSystem->TypingSystem : loadTarget()
	TypingSystem->TypingSystem : inputName()
	TypingSystem->TypingSystem : checkTime(name)
	group typing()
		TypingSystem->TypingSystem : checkEnd()
		TypingSystem->TypingSystem : inputLine()
		TypingSystem->TypingSystem : checkAns()
		TypingSystem->TypingSystem : getNextLine()
	TypingSystem-->TypingSystem : 
	end
	TypingSystem->TypingSystem : checkTime()
	TypingSystem->TypingSystem : finish()
end
@enduml
